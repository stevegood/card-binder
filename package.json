{
  "name": "card-binder",
  "version": "0.2.1",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/stevegood/card-binder.git",
  "scripts": {
    "prepublish": "yarn deploy-storybook && yarn build:production",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "watch": "cross-env NODE_ENV=development webpack -w",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:production": "cross-env NODE_ENV=production webpack -p",
    "lint": "cross-env NODE_ENV=development esw webpack.config.js src tools --color",
    "lint:watch": "yarn lint -- --watch",
    "major": "release-it major",
    "minor": "release-it minor",
    "patch": "release-it patch",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.10",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.7.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "release-it": "^2.8.5",
    "webpack": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^15.6.2",
    "react-dom": "^15.6.2"
  }
}
